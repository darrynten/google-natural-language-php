{
  "name": "darrynten/google-natural-language-php",
  "description": "PHP Google Natural Language Client with Extras",
  "type": "library",
  "require": {
    "php": "^5.5.9 || ^7.0",
    "guzzlehttp/guzzle": "^6.2.1",
    "google/cloud": "~0.56",
    "darrynten/any-cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0 || ~9.0",
    "mockery/mockery": "dev-master"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "DarrynTen\\GoogleNaturalLanguagePhp\\": "src/"
    },
    "exclude-from-classmap": ["/Tests/"]
  }
}
